{
  "name": "pqr-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "http-server -p 8080 -c-1 dist/",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev --deleteOutputPath=true --progress=false",
    "test": "ng test",
    "prettier": "prettier --check src/app/**/*.ts",
    "prettier:write": "prettier --write src/app/**/*.ts",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "lint": "ng lint && npm run lint:html && npm run lint:sass",
    "lint:fix": "ng lint --fix",
    "lint:html": "htmlhint src/app/**/**/*.html",
    "lint:sass": "stylelint src/app/**/*.sass --syntax sass",
    "e2e": "ng e2e",
    "coverage": "ng test --no-watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-buddies/prettier": "^1.0.0-alpha.3",
    "@angular/animations": "~9.1.8",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.1.8",
    "@angular/compiler": "~9.1.8",
    "@angular/core": "~9.1.8",
    "@angular/forms": "~9.1.8",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.1.8",
    "@angular/platform-browser-dynamic": "~9.1.8",
    "@angular/router": "~9.1.8",
    "@angular/service-worker": "~9.1.8",
    "@balticcode/ngx-hotkeys": "^2.1.3",
    "@ngrx/effects": "9.2.0",
    "@ngrx/entity": "9.2.0",
    "@ngrx/router-store": "^9.2.0",
    "@ngrx/store": "9.2.0",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@swimlane/ngx-datatable": "^16.1.1",
    "core-js": "^3.1.3",
    "crypto-js": "^4.0.0",
    "http-server": "^0.12.3",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "rxjs": "~6.5.4",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.9.1",
    "tslib": "^1.10.0",
    "url-template": "^2.0.8",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "~9.1.8",
    "@angular/compiler-cli": "~9.1.8",
    "@angular/language-service": "~9.1.8",
    "@ngrx/store-devtools": "9.2.0",
    "@types/faker": "^4.1.12",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/url-template": "^2.0.28",
    "codelyzer": "^5.1.2",
    "faker": "^4.1.0",
    "handlebars": "^4.5.0",
    "htmlhint": "^0.11.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-audit-ci-wrapper": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.3",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-standard": "^18.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
